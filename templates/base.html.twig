<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('style') }}
    {% endblock %}
</head>
<body>
<div uk-sticky="sel-target: .uk-navbar-container; cls-active: uk-navbar-sticky">
    <nav class="uk-navbar-container uk-box-shadow-medium" uk-navbar>
        <div class="uk-navbar-left">
            <ul class="uk-navbar-nav">
                <li><a class="" href="{{ path('dashboard') }}">Home</a></li>
                <li><a class="" href="{{ path('visits') }}">Visits</a></li>
                <li><a class="" href="{{ path('visitors') }}">Visitors</a></li>
                <li><a class="" href="{{ path('logger') }}">Logs</a></li>
            </ul>
        </div>
    </nav>
</div>
{% block body %}{% endblock %}
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    <script>
        window["onYopaLoaded"] = function (yopa) {
            yopa.idendity({{ app.user|json_encode }});
            yopa.page('{{ app.request.pathInfo }}');
        };
    </script>
{% endblock %}
</body>
</html>
